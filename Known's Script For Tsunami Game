-- [ If Ur Gonna Skid Or Steal Something ] --
-- [ Atlesat Give Credit To KnownZero ] --
-- [ Discord Server Link: https://discord.gg/DpPzVTHw5x ] --
-- [ The Uses And Time Are Infinite So Don't Ly ] --

local redzlib = loadstring(game:HttpGet("https://raw.githubusercontent.com/REDzHUB/RedzLibV5/main/Source.Lua"))()
local Window = redzlib:MakeWindow({Title = "Known's Hub", SubTitle = "Welcome User " .. game.Players.LocalPlayer.Name, SaveFolder = "DefaultConfig"})

-- Tabs
local MainTab = Window:MakeTab({"Main Tab"})
local FarmTab = Window:MakeTab({"Farm Tab"})
local FunTab = Window:MakeTab({"Fun Tab"})
local AntiTab = Window:MakeTab({"Antis Tab"})
local PlrTab = Window:MakeTab({"Player Tab"})

-- Variables
local coin = false
local afk = false
local banana = false

-- Buttons - Toggled - Sliders
FunTab:AddDropdown({
    Name = "Players List",
    Options = (function()
        local list = {}
        for _, player in ipairs(game.Players:GetPlayers()) do
            if player.Name ~= game.Players.LocalPlayer.Name then
                table.insert(list, player.Name)
            end
        end
        return list
    end)(),
    Default = "Please Pick A Player",
    Callback = function(v)
        selectedPlayer = v
    end
})

FunTab:AddButton({
    Name = "Force Kill",
    Default = false,
    Callback = function()
        if selectedPlayer then
            if not game.Players.LocalPlayer.Backpack:FindFirstChild("Lifeboat") then
                game:GetService("ReplicatedStorage").RemoteEvents.Gameplay.AttemptPurchase:InvokeServer("SurvivalShop_Lifeboat")
            end
        end
        game.Players.LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack:WaitForChild("Lifeboat"))
        repeat wait() until game.Players.LocalPlayer.Character:FindFirstChild("Lifeboat")
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-144.2454071044922, 119.5, -258.4832763671875)
        wait(0.5)
        game.Players.LocalPlayer.Character.Lifeboat.PlaceItem:FireServer(CFrame.new(-144.2454071044922, 119.5, -258.4832763671875), 2)
        wait(0.5)
        local lifeboat = workspace:WaitForChild("ActiveGearDebris"):WaitForChild("Lifeboats"):FindFirstChild("Lifeboat", 5)
        if lifeboat and lifeboat:FindFirstChild("OwnerUserId") and lifeboat.OwnerUserId.Value == game.Players.LocalPlayer.UserId then
            if lifeboat:FindFirstChildOfClass("VehicleSeat") then
                lifeboat:FindFirstChildOfClass("VehicleSeat"):Sit(game.Players.LocalPlayer.Character.Humanoid)
            end
        end
    end
})

FunTab:AddButton({
    Name = "Force Bring",
    Default = false,
    Callback = function()
        if selectedPlayer then
            
        end
    end
})

FunTab:AddButton({
    Name = "Force Win",
    Default = false,
    Callback = function()
        if selectedPlayer then
            
        end
    end
})

FarmTab:AddToggle({
    Name = "Afk Farmer",
    Default = false,
    Callback = function(v)
        afk = v
        if afk then
            local p = Instance.new("Part", game.Workspace)
            p.Size = Vector3.new(10, 0.1, 10)
            p.CFrame = CFrame.new(142.903061, 113.000336, -919.374451, -0.00148529548, -3.30771145e-05, -0.999998868, 5.60653498e-05, 1, -3.31604242e-05, 0.999998868, -5.61145425e-05, -0.00148529361)
            p.Anchored = true
            p.Material = Enum.Material.Neon
            p.Transparency = 0.8
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = p.CFrame + Vector3.new(0, p.Size.Y / 2 + game.Players.LocalPlayer.Character.HumanoidRootPart.Size.Y / 2, 0)
            while afk do
                for hue = 0, 1, 0.01 do
                    p.Color = Color3.fromHSV(hue, 1, 1)
                    wait(0.1)
                    if not afk then break end
                end
            end
            p:Destroy()
        end
    end
})

FarmTab:AddToggle({
    Name = "Coin Farmer",
    Default = false,
    Callback = function(v)
        coin = v
        if coin then
            while coin do
                for _, coinFolder in pairs(workspace:FindFirstChild("CurrentPointCoins"):GetChildren()) do
                    local coincollision = coinFolder:FindFirstChild("CoinCollision")
                    if coincollision then
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = coincollision.CFrame
                        wait()
                    end
                end
                wait()
            end
        end
    end
})

FarmTab:AddToggle({
    Name = "Chrome Banana Farmer",
    Default = false,
    Callback = function(v)
        banana = v
        local rg = workspace.Gravity
        while banana do
            workspace.Gravity = 0
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-188, -2093.5, -19.5) + Vector3.new(0, 3, 0)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(170, -2093.5, -19.5) + Vector3.new(0, 3, 0)
            wait()
        end
        workspace.Gravity = rg
    end
})

game.Players.LocalPlayer.CharacterAdded:Connect(function(c)
    c:WaitForChild("Humanoid").WalkSpeed = speedv
    c:WaitForChild("Humanoid").JumpPower = jumpv
end)

PlrTab:AddSlider({
    Name = "WalkSpeed",
    Min = 16,
    Max = 500,
    Default = 16,
    Callback = function(v)
        speedv = v
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
    end
})

PlrTab:AddSlider({
    Name = "JumpPower",
    Min = 60,
    Max = 500,
    Default = 60,
    Callback = function(v)
        jumpv = v
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = v
    end
})

game.Players.LocalPlayer.CharacterAdded:Connect(function(c)
    local hum = c:WaitForChild("Humanoid")
    hum.Died:Connect(function()
        while getgenv().TPWalk and game:GetService("RunService").Heartbeat:Wait() do
            if c:FindFirstChild("Humanoid") and hum.MoveDirection.Magnitude > 0 then
                c:TranslateBy(hum.MoveDirection * getgenv().TPSpeed)
            end
        end
    end)
end)

PlrTab:AddToggle({
    Name = "-- [ If Ur Gonna Skid Or Steal Something ] --
-- [ Atlesat Give Credit To KnownZero ] --
-- [ Discord Server Link: https://discord.gg/DpPzVTHw5x ] --
-- [ The Uses And Time Are Infinite So Don't Ly ] --

